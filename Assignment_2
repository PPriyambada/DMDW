    "import numpy as np\n",
    "x = np.arange(1,10,dtype = int)\n",
    "print(\"x+5: \",x+5)\n",
    "print(\"x-5: \",x-5)\n",
    "print(\"x/5: \",x/5)\n",
    "print(\"x*5\",x*5)\n",
    "print(\"x^2: \",np.power(x,2))\n",
    "print(\"x%2: \",np.mod(x,2))\n",
    "print(\"log(x): \",np.log(x))\n",
    "print(\"log2(x): \",np.log2(x))\n",
    "print(\"log10(x): \",np.log10(x))\n",
